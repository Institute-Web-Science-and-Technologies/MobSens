<h1>Users</h1>
<table class="table table-striped">
		<tr>
			<th>Created</th>
			<th>Last Sign in</th>
			<th>User</th>
		</tr>

		<% @users.each do |u| %>
		<tr>
			<td><%= u.created_at.strftime("%Y-%m-%d %H:%M") %></td>
			<td><%= u.last_sign_in_at.strftime("%Y-%m-%d %H:%M") %></td>
			<td><span class="glyphicon glyphicon-user"></span> <%= u.email %></td>
		</tr>
		<% end %>
</table>
