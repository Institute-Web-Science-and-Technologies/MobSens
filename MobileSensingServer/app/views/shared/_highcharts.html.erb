<div id="<%= caption %>_container" style="min-width: 310px; height: 400px; margin: 0 auto"></div><script type="text/javascript">	$(function() {$('#<%= caption %>_container').highcharts({chart : {zoomType: 'x'},title : {text : '<%= caption %>',},xAxis : {type: 'datetime'},yAxis : {title : {text : '<%= caption %> in <%= unit %>'}},credits: {      enabled: false},tooltip : {valueSuffix : '<%= unit %>'},legend : {layout : 'vertical',align : 'right',verticalAlign : 'middle',borderWidth : 0},				series : [
<% attributes.each do |attr| %>	{	name : '<%= attr %>',	data : [<% sensors.each do |sensor| %>		[Date.UTC(<%= sensor.time.strftime('%Y,%m,%d,%H,%M,%S,%L') %>),<%= eval('sensor.' + attr) %>],<% end %>]	},<% end %>]});});</script>