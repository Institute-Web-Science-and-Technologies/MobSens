<div id="map-canvas" style="height:600px; width:100%; margin: 0px; padding:0px;"></div><script>        var coordinates = [        <% locations.each do |location| %>                new google.maps.LatLng(<%= location.latitude %>, <%= location.longitude %>),        <% end %>        ];        google.maps.event.addDomListener(window, 'load', google_maps_init('map-canvas', <%= locations.first.latitude %>, <%= locations.first.longitude %>, coordinates));</script>